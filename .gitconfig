[user]
  name = Bob Black
  email = bob.black@sportngin.com
[color]
  ui = true
[alias]
  ap = add --patch
  ai = add -i
  rp = reset --patch
  b = branch
  co = checkout
  cop = checkout --patch
  ci = commit
  cv = commit -v
  ca = commit -v --amend
  d = diff
  do = diff --ours
  dt = diff --theirs
  f = fetch
  lp = log --patch
  nb = checkout --no-track -b
  p = push
  s = status
[push]
  default = current
