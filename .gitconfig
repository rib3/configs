[user]
  name = Bob Black
  email = bob.black@sportngin.com
[color]
  ui = true
[alias]
  ai = add -i
  ap = add --patch
  b = branch
  ca = commit -v --amend
  ci = commit
  co = checkout
  cop = checkout --patch
  cv = commit -v
  d = diff
  do = diff --ours
  dt = diff --theirs
  f = fetch
  lp = log --patch
  mm = "!git fetch && git merge --no-ff origin/master"
  nb = "!git fetch && git checkout --no-track -b"
  p = push
  r = reset
  rh = reset --hard
  rp = reset --patch
  s = status
  sl = stash list
[push]
  default = current
